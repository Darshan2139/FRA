# Copy to .env and fill values. Vite only exposes vars prefixed with VITE_.

# Choose one basemap provider (defaults to MapTiler if key present)
VITE_DEFAULT_BASEMAP=maptiler
VITE_MAPTILER_KEY=

# Optional alternatives (use instead of MapTiler)
VITE_MAPBOX_TOKEN=
VITE_GOOGLE_MAPS_API_KEY=
VITE_ARCGIS_API_KEY=

# App environment
VITE_APP_ENV=development
